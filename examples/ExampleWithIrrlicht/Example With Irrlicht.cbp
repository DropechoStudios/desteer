<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Example With Irrlicht" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Win32">
				<Option platforms="Windows;" />
				<Option output="bin\win32-gcc\example" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin\win32-gcc" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="lib\win32-gcc\libIrrlicht.dll.a" />
					<Add library="..\..\lib\libdesteer.a" />
				</Linker>
			</Target>
			<Target title="Linux">
				<Option platforms="Unix;" />
				<Option output="bin\linux\example" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin\linux" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-L/usr/X11R6/lib" />
					<Add option="-L../../lib/Linux" />
					<Add option="-lGL" />
					<Add option="-lXxf86vm" />
					<Add option="-lXext" />
					<Add option="-lX11" />
					<Add option="-ldl" />
					<Add option="-lpthread" />
					<Add library="..\..\lib\libdesteer.a" />
					<Add library="..\..\..\irrlicht\lib\Linux\libIrrlicht.a" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="include" />
			<Add directory="..\..\include" />
			<Add directory="..\irrlicht\include" />
		</Compiler>
		<Unit filename="include\BaseIncludes.hpp" />
		<Unit filename="include\BasicDemo.hpp" />
		<Unit filename="include\IrrlichtBaseEntity.hpp" />
		<Unit filename="include\IrrlichtMobileEntity.hpp" />
		<Unit filename="include\ObstacleAvoidanceDemo.hpp" />
		<Unit filename="include\ResPath.hpp" />
		<Unit filename="source\IrrlichtMobileEntity.cpp" />
		<Unit filename="source\main.cpp" />
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
